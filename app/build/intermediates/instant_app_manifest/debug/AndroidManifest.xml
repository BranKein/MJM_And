<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.brankein13.mjm"
    android:targetSandboxVersion="2"
    android:versionCode="114"
    android:versionName="1.14" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="27" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/mjm_ic_round"
        android:label="@string/app_name"
        android:roundIcon="@drawable/mjm_ic_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar" >
        <activity
            android:name="com.brankein13.mjm.MainActivity"
            android:windowSoftInputMode="adjustNothing" />
        <activity android:name="com.brankein13.mjm.Info" />
        <activity android:name="com.brankein13.mjm.Content" />
        <activity android:name="com.brankein13.mjm.Title" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="android.arch.lifecycle.VERSION"
            android:value="27.0.0-SNAPSHOT" />
    </application>

</manifest>